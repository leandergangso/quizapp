{
	"semi": false,
	"useTabs": true,
	"singleQuote": false,
	"trailingComma": "all",
	"singleAttributePerLine": true,
	"printWidth": 120,
	"plugins": [
		"prettier-plugin-svelte",
		"prettier-plugin-tailwindcss"
	],
	"overrides": [
		{
			"files": "*.svelte",
			"options": {
				"parser": "svelte"
			}
		}
	]
}